{
  "name": "clickalicious/caching-middleware",
  "description": "The PSR-7 middleware for caching compatible to PSR-6.",
  "type": "library",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "roave/security-advisories": "dev-master as 1.0.x-dev",
    "shrikeh/teapot": "^2.3",
    "psr/http-message": "^1.0",
    "gpupo/cache": "^1.3",
    "wandu/http": "^3.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "relay/relay": "^1.1",
    "cocur/slugify": "^2.1",
    "codacy/coverage": "^1.0.5",
    "symfony/var-dumper": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Clickalicious\\Caching\\Middleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clickalicious\\Caching\\Middleware\\": "tests/"
    }
  },
  "keywords": [
    "psr-7",
    "middleware",
    "cache",
    "caching",
    "relay",
    "stack",
    "psr-6"
  ],
  "homepage": "https://github.com/clickalicious/caching-middleware",
  "license": "MIT",
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/caching-middleware/issues",
    "wiki": "https://github.com/clickalicious/caching-middleware/wiki",
    "tests": "https://clickalicious.github.io/caching-middleware"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  }
}
